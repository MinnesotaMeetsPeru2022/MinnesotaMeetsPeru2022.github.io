{
    "name": "vite-react-typescript-eslint-prettier",
    "version": "0.0.1",
    "scripts": {
        "start": "npm run dev",
        "dev": "vite",
        "build": "tsc && vite build",
        "serve": "vite preview",
        "fix": "npm run lint:format && npm run lint:eslint ",
        "lint:eslint": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
        "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
        "type-check": "tsc"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "@typescript-eslint/eslint-plugin": "^5.29.2",
        "@typescript-eslint/parser": "^5.10.2",
        "@vitejs/plugin-react": "^1.3.2",
        "eslint": "^8.18.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "pre-commit": "^1.2.2",
        "prettier": "^2.5.1",
        "ts-jest": "^28.0.5",
        "ts-node": "^10.8.1",
        "typescript": "^4.6.3",
        "vite": "^2.9.12",
        "vite-tsconfig-paths": "^3.5.0"
    },
    "pre-commit": "lint",
    "license": "MIT"
}
